{
  "name": "codexten/yii-core",
  "description": "Yii framework extended for Codexten",
  "keywords": [
    "yii2",
    "codexten"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "yiisoft/yii2": "~2.0.16||~2.0.0",
    "vlucas/phpdotenv": "v2.5.1",
    "codexten/docs": "~2.0.0",
    "codexten/yii-widget-select2": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "params": "src/config/params.php",
      "required": "src/config/required.php",
      "defines": "src/config/defines.php",
      "i18n": "src/config/i18n.php",
      "mailer": "src/config/mailer.php",
      "base": "src/config/base.php",
      "core": [
        "$base",
        "src/config/core.php"
      ],
      "migrationNamespaces": "src/config/migrationNamespaces.php",
      "console": "src/config/console.php",
      "grid": "src/grid/config/grid.php"
    }
  }
}
