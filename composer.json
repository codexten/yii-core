{
  "name": "codexten/yii-core",
  "description": "Yii framework extended for Codexten",
  "keywords": [
    "yii2",
    "codexten"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "yiisoft/yii2": "2.0.16",
    "vlucas/phpdotenv": "v2.5.1",
    "codexten/docs": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "params": "src/config/params.php",
      "i18n": "src/config/i18n.php",
      "mailer": "src/config/mailer.php",
      "core": "src/config/core.php"
    }
  }
}
